rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if request.auth != null;
      allow write, delete: if false;
    }
    match /teams/{document=**} {
      allow read: if true;
    }
    match /users/{document=**} {
      allow create, read: if true;
    }
  }
}
